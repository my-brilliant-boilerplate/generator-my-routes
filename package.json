{
  "name": "generator-my-routes",
  "version": "0.0.0",
  "description": "Yeoman generator for routes in React project",
  "homepage": "https://github.com/my-brilliant-boilerplate/generator-my-routes",
  "author": {
    "name": "AlekseyLeshko",
    "email": "alekseyleshko@gmail.com",
    "url": "https://github.com/AlekseyLeshko"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "yeoman",
    "yeoman-generator",
    "generator",
    "routes",
    "route",
    "react",
    "boilerplate"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.20.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "lint-staged": "^6.1.0",
    "nsp": "^3.1.0",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "chalk": "^2.3.1",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "coverageReporters": ["html", "lcov"]
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "AlekseyLeshko/generator-my-routes",
  "license": "MIT"
}
